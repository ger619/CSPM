

<div class="p-4 border-white rounded-lg w-full">
<div class="relative overflow-x-auto w-full">
  <table class="w-full text-sm text-center rtl:text-right text-gray-500 dark:text-gray-400">
    <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
    <tr>
      <th scope="col" class="px-6 py-3 text-left">
        Name of The Project
      </th>
      <th scope="col" class="px-6 py-3">
       Total Number Of Tickets Available
      </th>
      <th scope="col" class="px-6 py-3">
        Number of Issues Published
      </th>
    </tr>
    </thead>
    <tbody>
    <% @issues_count_per_project_for_current_user.each do |project_title, issue_count| %>

      <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 text-center">
        <th scope="row" class="text-left capitalize px-6 py-4 font-medium whitespace-nowrap ">
            <%= project_title %>
        </th>
        <td class="px-6 py-4">
            <%= @tickets_count_per_project[project_title] %>
        </td>
        <td>
            <%= issue_count %>
        </td>

      </tr>

    <% end %>
    </tbody>

  </table>
</div>
</div>
