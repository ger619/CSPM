<div class="p-8 sm:ml-64">
  <div role="status" class="space-y-8  md:space-y-0 md:space-x-8 rtl:space-x-reverse md:flex md:items-center">
  <div class="flex items-center justify-center w-full h-48 bg-gray-300 rounded sm:w-96 dark:bg-gray-700">

<% if @ticket.image.present? %>
    <%= image_tag @ticket.image, class: 'w-12 h-12 rounded-full animate-pulse' %>
      <% else %>
      <svg class="w-10 h-10 text-gray-200 dark:text-gray-600" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 20 18">
        <path d="M18 0H2a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h16a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2Zm-5.5 4a1.5 1.5 0 1 1 0 3 1.5 1.5 0 0 1 0-3Zm4.376 10.481A1 1 0 0 1 16 15H4a1 1 0 0 1-.895-1.447l3.5-7A1 1 0 0 1 7.468 6a.965.965 0 0 1 .9.5l2.775 4.757 1.546-1.887a1 1 0 0 1 1.618.1l2.541 4a1 1 0 0 1 .028 1.011Z"/>
      </svg>


    <% end %>


  </div>
  <div class="w-full">
    <%= @ticket.id %><br>
    Parent ID:
    <%= @ticket.parent_id %>
    <br>
    <%= @project.id %>

    <div class="h-2.5 rounded-full dark:bg-gray-700 w-48 mb-4"><%= @ticket.issue %></div>
    <div class="h-2  rounded-full dark:bg-gray-700 max-w-[480px] mb-2.5"><%= @ticket.priority %></div>
    <div class="h-2 rounded-full dark:bg-gray-700 mb-2.5"><%= @ticket.body %></div>
    <% if @ticket.parent_id.present? %>
      <div class="h-2.5 rounded-full dark:bg-gray-700 w-48 mb-4"><%= @ticket.issue %></div>
      <div class="h-2  rounded-full dark:bg-gray-700 max-w-[480px] mb-2.5"><%= @ticket.priority %></div>
      <div class="h-2 rounded-full dark:bg-gray-700 mb-2.5"><%= @ticket.body %></div>

    <% end %>

  </div>


    <%#= render partial: 'tickets/ticket' %>

  <span class="sr-only">Loading...</span>
</div>
</div>
<%= @ticket %>

